    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Current Extension file with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/extensions",
            "args": "${command:pickArgs}"
        }
    ]
}